{
    "name": "Web Staller",
    "version": "1.0",
    "description": "Puts an input lag on each website you visit.",
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "storage",
        "tabs",
        "*://*/*"
    ],
    "background" : {
        "scripts":["background.js"]
    },
    "page_action": {
        "default_popup": "popup.html"
    },
    "manifest_version": 2
}