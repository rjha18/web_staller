{
    "name": "Web Staller",
    "version": "1.0",
    "description": "Puts an input lag on each website you visit.",
    "permissions": [
        "activeTab",
        "declarativeContent",
        "storage"
    ],
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": true
    },
    "page_action": {
        "default_popup": "popup.html"
    },
    "manifest_version": 2
}